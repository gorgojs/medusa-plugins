{
  "name": "@gorgo/medusa-feed-yandex",
  "version": "0.2.6",
  "description": "A Medusa plugin that generates a product feed in YML (Yandex Market Language) format",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gorgojs/medusa-plugins",
    "directory": "packages/medusa-feed-yandex"
  },
  "files": [
    ".medusa/server",
    "integration-tests",
    "src"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": "./.medusa/server/src/modules/*/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./*": "./.medusa/server/src/*.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa",
    "medusa-v2",
    "medusa-plugin",
    "medusa-plugin-integration",
    "medusa-plugin-other",
    "ecommerce",
    "yandex",
    "yandex market",
    "yml",
    "yml feed",
    "product feed",
    "export",
    "catalog export",
    "yandex yml",
    "marketplace"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "yarn predev && medusa plugin:develop",
    "predev": "medusa plugin:publish",
    "prepublishOnly": "medusa plugin:build"
  },
  "dependencies": {
    "@gorgo/medusa-feed": "file:.yalc/@gorgo/medusa-feed",
    "yandex-market-language": "^0.5.0"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "^2.8.0",
    "@medusajs/cli": "^2.8.0",
    "@medusajs/framework": "^2.8.0",
    "@medusajs/icons": "^2.8.0",
    "@medusajs/medusa": "^2.8.0",
    "@medusajs/test-utils": "^2.8.0",
    "@medusajs/ui": "^4.0.9",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.13.17",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.2.25",
    "awilix": "^8.0.1",
    "jest": "^29.7.0",
    "pg": "^8.14.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.8.0",
    "@medusajs/cli": "^2.8.0",
    "@medusajs/framework": "^2.8.0",
    "@medusajs/icons": "^2.8.0",
    "@medusajs/medusa": "^2.8.0",
    "@medusajs/test-utils": "^2.8.0",
    "@medusajs/ui": "^4.0.9",
    "@mikro-orm/cli": "^6.4.3",
    "@mikro-orm/core": "^6.4.3",
    "@mikro-orm/knex": "^6.4.3",
    "@mikro-orm/migrations": "^6.4.3",
    "@mikro-orm/postgresql": "^6.4.3",
    "awilix": "^8.0.1",
    "date-fns": "^3.6.0",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "react-i18next": "^13.5.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20"
  }
}
