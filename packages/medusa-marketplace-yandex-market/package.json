{
  "name": "@gorgo/medusa-marketplace-yandex-market",
  "version": "0.0.1",
  "description": "Medusa plugin for integration with Yandex Market.",
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": "./.medusa/server/src/modules/*/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./*": "./.medusa/server/src/*.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa",
    "plugin",
    "medusa-plugin-other",
    "medusa-v2",
    "medusa-plugin",
    "medusa-plugin-integration",
    "yandex-marketplace",
    "yandex-market",
    "marketplace",
    "medusa-marketplace"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "yarn predev && medusa plugin:develop",
    "openapi:bundle": "OAS_PATH=openapi/yandex-market-partner-api && redocly bundle $OAS_PATH/openapi/openapi.yaml --output openapi/upstream.yaml --ext yaml",
    "openapi:gen": "OAC_PATH=src/lib/yandex-market-client && rm -rf $OAC_PATH && openapi-generator-cli generate -i /local/openapi/upstream.yaml -g typescript-axios -o /local/$OAC_PATH --skip-validate-spec --additional-properties=useSingleRequestParameter=true",
    "openapi:pull": "OAS_PATH=openapi/yandex-market-partner-api && rm -rf $OAS_PATH && git clone --depth=1 https://github.com/yandex-market/yandex-market-partner-api.git $OAS_PATH && yarn openapi:bundle && git -C $OAS_PATH rev-parse HEAD > openapi/SPEC_VERSION.txt",
    "prepublishOnly": "yarn openapi:gen && medusa plugin:build",
    "predev": "medusa plugin:publish"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "axios-rate-limit": "^1.4.0",
    "axios-retry": "^4.5.0"
  },
  "devDependencies": {
    "@gorgo/medusa-marketplace": "*",
    "@medusajs/admin-sdk": "^2.11.2",
    "@medusajs/cli": "^2.11.2",
    "@medusajs/framework": "^2.11.2",
    "@medusajs/icons": "^2.11.2",
    "@medusajs/medusa": "^2.11.2",
    "@medusajs/test-utils": "^2.11.2",
    "@medusajs/ui": "^4.0.25",
    "@openapitools/openapi-generator-cli": "^2.27.0",
    "@swc/core": "^1.5.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@gorgo/medusa-marketplace": "*",
    "@medusajs/admin-sdk": "^2.11.2",
    "@medusajs/cli": "^2.11.2",
    "@medusajs/framework": "^2.11.2",
    "@medusajs/icons": "^2.11.2",
    "@medusajs/medusa": "^2.11.2",
    "@medusajs/test-utils": "^2.11.2",
    "@medusajs/ui": "^4.0.25"
  },
  "engines": {
    "node": ">=20"
  }
}
