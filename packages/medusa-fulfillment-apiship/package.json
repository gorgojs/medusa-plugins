{
  "name": "@gorgo/medusa-fulfillment-apiship",
  "version": "0.1.1",
  "description": "Apiship fulfillment provider for Medusa",
  "files": [
    ".medusa/server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gorgojs/medusa-plugins",
    "directory": "packages/medusa-fulfillment-apiship"
  },
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": "./.medusa/server/src/modules/*/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    },
    "./*": "./.medusa/server/src/*.js"
  },
  "keywords": [
    "medusa",
    "medusa-v2",
    "medusa-plugin",
    "medusa-plugin-integration",
    "medusa-plugin-fulfillment",
    "medusa-fulfillment",
    "medusa-fulfillment-apiship",
    "apiship",
    "apiship-fulfillment",
    "shipping",
    "shipment"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "yarn predev && medusa plugin:develop",
    "openapi:gen": "OAC_PATH=src/lib/apiship-client && rm -rf $OAC_PATH && openapi-generator-cli generate -i /local/openapi/upstream.yaml -g typescript-axios -o /local/$OAC_PATH --skip-validate-spec --additional-properties=useSingleRequestParameter=true",
    "openapi:pull": "rm -rf openapi && mkdir -p openapi && curl -L https://api.apiship.ru/doc/openapi.yaml -o openapi/upstream.yaml && yarn openapi:version",
    "openapi:version": "shasum -a 256 openapi/upstream.yaml | awk '{print $1}' > openapi/SPEC_VERSION.txt && echo 'Apiship spec updated:' && cat openapi/SPEC_VERSION.txt",
    "predev": "medusa plugin:publish",
    "prepublishOnly": "yarn openapi:gen && medusa plugin:build"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "axios-rate-limit": "^1.4.0",
    "axios-retry": "^4.5.0"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "^2.7.1",
    "@medusajs/cli": "^2.7.1",
    "@medusajs/framework": "^2.7.1",
    "@medusajs/icons": "^2.7.1",
    "@medusajs/medusa": "^2.7.1",
    "@medusajs/test-utils": "^2.7.1",
    "@medusajs/ui": "^4.0.8",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@openapitools/openapi-generator-cli": "^2.25.0",
    "@swc/core": "1.5.7",
    "@types/node": "^22.13.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "awilix": "^8.0.1",
    "pg": "^8.14.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.7.1",
    "@medusajs/cli": "^2.7.1",
    "@medusajs/framework": "^2.7.1",
    "@medusajs/icons": "^2.7.1",
    "@medusajs/medusa": "^2.7.1",
    "@medusajs/test-utils": "^2.7.1",
    "@medusajs/ui": "^4.0.8",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "awilix": "^8.0.1",
    "pg": "^8.14.1"
  },
  "engines": {
    "node": ">=20"
  }
}
