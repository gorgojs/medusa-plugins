{
  "name": "medusa",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "yarn predev && yarn && medusa develop",
    "predev": "medusa plugin:add @gorgo/medusa-marketplace && medusa plugin:add @gorgo/medusa-marketplace-wildberries",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@gorgo/medusa-marketplace": "file:.yalc/@gorgo/medusa-marketplace",
    "@gorgo/medusa-marketplace-wildberries": "file:.yalc/@gorgo/medusa-marketplace-wildberries",
    "@medusajs/admin-sdk": "2.12.2",
    "@medusajs/cli": "2.12.2",
    "@medusajs/framework": "2.12.2",
    "@medusajs/medusa": "2.12.2"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.2",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
